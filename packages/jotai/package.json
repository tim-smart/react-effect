{
  "name": "@react-effect/jotai",
  "version": "0.1.3",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "./dist/**/*"
  ],
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "default": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "scripts": {
    "clean": "yarn react-effect:clean",
    "build": "yarn react-effect:tsc -b tsconfig.json",
    "prepublishOnly": "yarn clean && yarn build"
  },
  "packageManager": "yarn@3.3.0",
  "peerDependencies": {
    "@effect/core": "*",
    "jotai": "*"
  },
  "devDependencies": {
    "@effect/core": "^0.0.16",
    "jotai": "^1.9.2"
  },
  "gitHead": "249dab5190e3c98b6c25ceeb835d7d3a5fb11074"
}
