  "scripts": {
    "clean": "yarn react-effect:clean",
    "build": "yarn react-effect:build",
    "prepublishOnly": "yarn clean && yarn build"
  },
  "packageManager": "yarn@3.3.0",
  "peerDependencies": {
    "@effect/core": "*",
    "jotai": "*"
  },
  "devDependencies": {
    "@effect/core": "^0.0.16",
    "jotai": "^1.9.2"
  },
  "gitHead": "249dab5190e3c98b6c25ceeb835d7d3a5fb11074"
}
