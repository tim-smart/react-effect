{
  "name": "@react-effect/jotai",
  "version": "0.2.0",
  "type": "module",
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "exports": {
    ".": {
      "default": "./index.js",
      "types": "./index.d.ts"
    },
    "./*": {
      "default": "./*.js",
      "types": "./*.d.ts"
    }
