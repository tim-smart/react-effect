{
  "name": "@react-effect/react",
  "version": "0.1.2",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "./dist/**/*"
  ],
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "default": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "scripts": {
    "clean": "yarn react-effect:clean",
    "build": "yarn react-effect:tsc -b tsconfig.json",
    "prepublishOnly": "yarn clean && yarn build"
  },
  "packageManager": "yarn@3.3.0",
  "peerDependencies": {
    "react": "*"
  },
  "devDependencies": {
    "@types/react": "^18.0.25",
    "react": "^18.2.0"
  },
  "dependencies": {
    "@react-effect/jotai": "^0.1.2"
  },
  "gitHead": "249dab5190e3c98b6c25ceeb835d7d3a5fb11074"
}
